<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        style="@style/context_layout">
        <com.joint.turman.customwidget.section.ReadableSection
            android:id="@+id/frg_proinfo_name"
            style="@style/context_section"
            app:label_text="项目名称:"
            />
        <com.joint.turman.customwidget.section.ReadableSection
            android:id="@+id/frg_proinfo_type"
            style="@style/context_section"
            app:label_text="项目类别:"
            />
        <com.joint.turman.customwidget.section.ReadableSection
            android:id="@+id/frg_proinfo_client"
            app:isLink="true"
            style="@style/context_section"
            app:label_text="客户信息:"
            />
        <com.joint.turman.customwidget.section.ReadableSection
            android:id="@+id/frg_proinfo_linkman"
            style="@style/context_section"
            app:isLink="true"
            app:label_text="联系人:"
            />
        <com.joint.turman.customwidget.section.ReadableSection
            android:id="@+id/frg_proinfo_area"
            style="@style/context_section"
            app:label_text="所在地区:"
            />
        <com.joint.turman.customwidget.section.ReadableSection
            android:id="@+id/frg_proinfo_address"
            style="@style/context_section"
            app:label_text="项目地址:"
            />
        <com.joint.turman.customwidget.section.ReadableSection
            android:id="@+id/frg_proinfo_chief"
            style="@style/context_section"
            app:label_text="项目总监:"
            />
        <com.joint.turman.customwidget.section.ReadableSection
            android:id="@+id/frg_proinfo_group"
            style="@style/context_section"
            app:label_text="项目组员:"
            />
        <com.joint.turman.customwidget.section.ReadableSection
            android:id="@+id/frg_proinfo_start"
            style="@style/context_section"
            app:label_text="启动时间:"
            />
        <com.joint.turman.customwidget.section.ReadableSection
            android:id="@+id/frg_proinfo_end"
            style="@style/context_section"
            app:label_text="完成时间:"
            />
        <com.joint.turman.customwidget.section.ReadableSection
            android:id="@+id/frg_proinfo_creater"
            style="@style/context_section"
            app:label_text="创建人:"
            />
    </LinearLayout>
</ScrollView>